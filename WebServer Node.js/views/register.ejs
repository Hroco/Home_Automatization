 <div id="maincontent" class="container">

<form action="/register" method="POST">
	<div class="cbi-map">
		<h2><a id="content" name="content">Registration</a></h2>
		<div class="cbi-map-descr">
			Please enter your personal data.</div>
		<fieldset class="cbi-section"><fieldset class="cbi-section-node">
			<div class="cbi-value">
				<label class="cbi-value-title">Username</label>
				<div class="cbi-value-field">
					<input class="cbi-input-user" type="text" name="name"/>
				</div>
      </div>
      <div class="cbi-value">
				<label class="cbi-value-title">Email</label>
				<div class="cbi-value-field">
					<input class="cbi-input-user" type="email" name="email"/>
				</div>
      </div>
      <div class="cbi-value">
				<label class="cbi-value-title">PhoneNumber</label>
				<div class="cbi-value-field">
					<input class="cbi-input-user" type="text" name="phone"/>
				</div>
      </div>
			<div class="cbi-value cbi-value-last">
				<label class="cbi-value-title">Password</label>
				<div class="cbi-value-field">
					<input class="cbi-input-password" type="password" name="password" />
				</div>
			</div>
		</fieldset></fieldset>
	</div>

	<div>
		<input type="submit" value="Register" class="cbi-button cbi-button-apply" />
		<a href="/login" class="cbi-button cbi-button-reset">Login</a>
	</div>
</form>
